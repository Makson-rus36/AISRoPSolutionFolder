<Window x:Class="WindowEditData.WindowEdit"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WindowEditData"
        mc:Ignorable="d"
        Title="WindowEdit" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition Height="50"></RowDefinition>
        </Grid.RowDefinitions>
        <StackPanel x:Name="MainPanel" Grid.Row="0"></StackPanel>
        <Button x:Name="btnOK" Grid.Row="1" HorizontalAlignment="Right" Content="Готово" FontSize="14" VerticalAlignment="Center" Click="BtnOK_OnClick"></Button>
    </Grid>
</Window>
